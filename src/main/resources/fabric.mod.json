{
	"schemaVersion": 1,
	"id": "bouncylife",
	"version": "${version}",

	"name": "Bouncy Life",
	"description": "Bounce through your life",
	"authors": [
		"Siphalor"
	],

	"license": "Apache-2.0",
	"icon": "assets/bouncylife/icon.png",

	"environment": "*",
	"entrypoints": {
		"main": [
			"de.siphalor.bouncylife.BouncyLife"
		],
		"client": [
			"de.siphalor.bouncylife.client.BouncyLifeClient"
		],
		"tweed4:config": [
			"de.siphalor.bouncylife.BLConfig"
		],
		"mm:early_risers": [
			"de.siphalor.bouncylife.BLEarlyRiser"
		]
	},
	"mixins": [
		"bouncylife.mixins.json"
	],

	"requires": {
		"fabricloader": ">=0.4.0",
		"fabric": "*"
	},
	"suggests": {
		"pushtocraft": "*",
		"modmenu": "*"
	}
